#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hubertmalkowski/RubymineProjects/blog_docker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hubertmalkowski/RubymineProjects/blog_docker/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@storybook+vue@6.5.15_lombgfyrt2apfam7lltpeyeg6y/node_modules/@storybook/vue/bin/build.js" "$@"
else
  exec node  "$basedir/../../../../../../@storybook+vue@6.5.15_lombgfyrt2apfam7lltpeyeg6y/node_modules/@storybook/vue/bin/build.js" "$@"
fi
