#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hubertmalkowski/RubymineProjects/blog_docker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hubertmalkowski/RubymineProjects/blog_docker/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jscodeshift@0.13.1/node_modules/jscodeshift/bin/jscodeshift.js" "$@"
else
  exec node  "$basedir/../../../../../jscodeshift@0.13.1/node_modules/jscodeshift/bin/jscodeshift.js" "$@"
fi
